[general]
configs_folder=/opt/janus/etc/janus
plugins_folder=/opt/janus/lib/janus/plugins
debug_level=4

[webserver]
base_path=/janus
threads=unlimited
http=no
https=yes
secure_port=8089

[admin]
admin_base_path=/admin
admin_threads=unlimited
admin_http=no
admin_https=yes
admin_secure_port=7889
; admin_acl=127.0.0.1

[certificates]
cert_pem=/opt/janus/certs/janus.pem
cert_key=/opt/janus/certs/janus.key
